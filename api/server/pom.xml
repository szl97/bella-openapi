    <build>
        <finalName>${project.parent.artifactId}</finalName>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-deploy-plugin</artifactId>
                <configuration>
                    <skip>true</skip>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>build-helper-maven-plugin</artifactId>
                <executions>
                    <execution>
                        <id>add-source</id>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>add-source</goal>
                        </goals>
                        <configuration>
                            <sources>
                                <source>src/codegen/java</source>
                            </sources>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <!-- 代码生成器插件 -->
            <plugin>
                <groupId>org.jooq</groupId>
                <artifactId>jooq-codegen-maven</artifactId>
                <version>${jooq.version}</version>
                <configuration>
                    <jdbc>
                        <driver>${db.driver}</driver>
                        <url>${db.url}</url>
                        <user>${db.username}</user>
                        <password>${db.password}</password>
                    </jdbc>
                    <generator>
                        <database>
                            <inputSchema>${db.schema}</inputSchema>
                            <outputSchemaToDefault>true</outputSchemaToDefault>
                            <includes>.*</includes>
                            <unsignedTypes>false</unsignedTypes>
                        </database>
                        <strategy>
                            <name>org.jooq.codegen.DefaultGeneratorStrategy</name>
                            <matchers>
                                <tables>
                                    <table>
                                        <daoClass>
                                            <transform>PASCAL</transform>
                                            <expression>$0_D_B_Dao</expression>
                                        </daoClass>
                                        <pojoClass>
                                            <transform>PASCAL</transform>
                                            <expression>$0_D_B</expression>
                                        </pojoClass>
                                        <pojoImplements>io.github.bella-top.db.repo.Timed</pojoImplements>
                                        <recordImplements>io.github.bella-top.db.repo.Timed</recordImplements>
                                        <expression>user</expression>
                                    </table>
                                    <table>
                                        <daoClass>
                                            <transform>PASCAL</transform>
                                            <expression>$0_D_B_Dao</expression>
                                        </daoClass>
                                        <pojoClass>
                                            <transform>PASCAL</transform>
                                            <expression>$0_D_B</expression>
                                        </pojoClass>
                                        <pojoImplements>io.github.bella-top.db.repo.Timed</pojoImplements>
                                        <recordImplements>io.github.bella-top.db.repo.Timed</recordImplements>
                                        <expression>apikey_month_cost</expression>
                                    </table>
                                    <table>
                                        <daoClass>
                                            <transform>PASCAL</transform>
                                            <expression>$0_D_B_Dao</expression>
                                        </daoClass>
                                        <pojoClass>
                                            <transform>PASCAL</transform>
                                            <expression>$0_D_B</expression>
                                        </pojoClass>
                                        <pojoImplements>io.github.bella-top.db.repo.Timed</pojoImplements>
                                        <recordImplements>io.github.bella-top.db.repo.Timed</recordImplements>
                                        <expression>space</expression>
                                    </table>
                                    <table>
                                        <daoClass>
                                            <transform>PASCAL</transform>
                                            <expression>$0_D_B_Dao</expression>
                                        </daoClass>
                                        <pojoClass>
                                            <transform>PASCAL</transform>
                                            <expression>$0_D_B</expression>
                                        </pojoClass>
                                        <pojoImplements>io.github.bella-top.db.repo.Timed</pojoImplements>
                                        <recordImplements>io.github.bella-top.db.repo.Timed</recordImplements>
                                        <expression>space_role</expression>
                                    </table>
                                    <table>
                                        <daoClass>
                                            <transform>PASCAL</transform>
                                            <expression>$0_D_B_Dao</expression>
                                        </daoClass>
                                        <pojoClass>
                                            <transform>PASCAL</transform>
                                            <expression>$0_D_B</expression>
                                        </pojoClass>
                                        <pojoImplements>io.github.bella-top.db.repo.Timed</pojoImplements>
                                        <recordImplements>io.github.bella-top.db.repo.Timed</recordImplements>
                                        <expression>space_member</expression>
                                    </table>
                                    <table>
                                        <daoClass>
                                            <transform>PASCAL</transform>
                                            <expression>$0_D_B_Dao</expression>
                                        </daoClass>
                                        <pojoClass>
                                            <transform>PASCAL</transform>
                                            <expression>$0_D_B</expression>
                                        </pojoClass>
                                        <pojoImplements>io.github.bella-top.db.repo.Operator</pojoImplements>
                                        <recordImplements>io.github.bella-top.db.repo.Operator</recordImplements>
                                    </table>
                                </tables>
                            </matchers>
                        </strategy>
                        <generate>
                            <deprecated>false</deprecated>
                            <records>true</records>
                            <pojos>true</pojos>
                            <daos>false</daos>
                        </generate>
                        <target>
                            <packageName>io.github.bella-top</packageName>
                            <directory>src/codegen/java</directory>
                            <encoding>UTF-8</encoding>
                        </target>
                    </generator>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
            </plugin>
        </plugins>
    </build>
